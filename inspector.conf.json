{
    "mcpServers": {
        "PowerSearch MCP (STDIO)": {
            "command": "uv",
            "args": [
                "run",
                "fastmcp",
                "run",
                "fastmcp.json",
                "--skip-source",
                "--skip-env"
            ]
        },
        "PowerSearch MCP (HTTP)": {
            "type": "streamable-http",
            "url": "http://localhost:8099/mcp"
        }
    }
}
